{
  "name": "vscode-dt0",
  "displayName": "vscode-dt0",
  "description": "DT0 language highlighter",
  "version": "0.0.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DiscreteTom/vscode-dt0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dt0",
        "aliases": [
          "DT0",
          "dt0"
        ],
        "extensions": [
          "dt0"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dt0",
        "scopeName": "source.dt0",
        "path": "./syntaxes/dt0.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@discretetom/r-compose": "^0.2.1",
    "@types/node": "^20.9.0",
    "tmlb": "^0.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
